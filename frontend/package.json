{
  "name": "@satyashield/frontend",
  "version": "1.0.0",
  "description": "SatyaShield Frontend Application",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false --maxWorkers=2",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "analyze": "ANALYZE=true next build",
    "docker:build": "docker build -f ../deployment/docker/frontend.Dockerfile -t satyashield/frontend .",
    "docker:run": "docker run -p 3000:3000 satyashield/frontend",
    "security:audit": "npm audit",
    "bundle-analyzer": "ANALYZE=true npm run build"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
